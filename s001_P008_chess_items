import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

public class Main {
	public static int readInt(StringTokenizer st) {
		return Integer.parseInt(st.nextToken());
	}
	public static void main(String[] args) throws Exception {
		int king_org = 1, queen_org = 1, rook_org = 2, bishop_org = 2, knight_org = 2, pawn_org = 8;
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		StringTokenizer st = new StringTokenizer(br.readLine());
		int king = readInt(st);
		int queen = readInt(st);
		int rook = readInt(st);
		int bishop = readInt(st);
		int knight = readInt(st);
		int pawn = readInt(st);
		System.out.printf("%d %d %d %d %d %d", king_org - king, queen_org - queen, rook_org - rook, bishop_org - bishop, knight_org - knight, pawn_org - pawn);
	}
}

